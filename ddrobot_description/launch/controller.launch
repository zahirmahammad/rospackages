<launch>

<rosparam file="$(find ddrobot_description)/launch/armcontrol.yaml" command="load"/>
<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="ddrobot" args="Rev4_position_controller Rev5_position_controller Rev6_position_controller joint_state_controller "/>
<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
  <remap from="/joint_states" to="/ddrobot/joint_states"/>
</node>

</launch>